{
  "name": "@marktext/muya",
  "version": "0.1.13",
  "description": "Future markdown editor for web and desktop application development",
  "main": "built/index.js",
  "module": "built/index.js",
  "scripts": {
    "dev": "webpack serve --config ./example/webpack.config.js --open",
    "build": "tsc",
    "copyfiles": "copyfiles -u 1 lib/**/*.css built && copyfiles -u 1 lib/**/*.json built && copyfiles -u 1 lib/**/*.png built && copyfiles -u 1 lib/**/*.svg built && copyfiles -u 1 lib/**/*.ttf built && copyfiles -u 1 lib/**/*.woff built && copyfiles -u 1 lib/**/*.woff2 built",
    "test": "jest --env=jsdom",
    "lint": "eslint lib",
    "lint:fix": "eslint --fix lib",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "pub": "npm publish --access public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marktext/muya.git"
  },
  "keywords": [
    "markdown",
    "editor",
    "web",
    "desktop"
  ],
  "author": "ransixi@gmail.com",
  "license": "MIT",
